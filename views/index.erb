	<!-- start book -->
	<section class="container">
		<header><h1>Book</h1></header>
		<div class="row">
			<div class="span12">

				<table id="rates" class='table'>
					<thead>
						<tr>
							<th>Season</th>
							<th>2 nights</th>
							<th>3 nights</th>
							<th>4 nights</th>
							<th>5 nights</th>
							<th>6 nights</th>
							<th>7 nights</th>
						</tr>
					</thead>
					<tbody>
						<tr>
							<td>New Year</td>
							<td>-</td>
							<td>-</td>
							<td>-</td>
							<td>-</td>
							<td>-</td>
							<td>$2,205</td>
						</tr>

						<tr>
							<td>Summer Holidays</td>
							<td>-</td>
							<td>-</td>
							<td>-</td>
							<td>-</td>
							<td>-</td>
							<td>$1,890</td>
						</tr>

						<tr>
							<td>Summer Shoulder (Dec & Feb)</td>
							<td>$530</td>
							<td>$750</td>
							<td>$960</td>
							<td>$1,150</td>
							<td>$1,320</td>
							<td>$1,540</td>
						</tr>

						<tr>
							<td>Autumn & Spring</td>
							<td>$490</td>
							<td>$690</td>
							<td>$880</td>
							<td>$1,050</td>
							<td>$1,200</td>
							<td>$1,330</td>
						</tr>

						<tr>
							<td>Winter</td>
							<td>$440</td>
							<td>$585</td>
							<td>$740</td>
							<td>$875</td>
							<td>$990</td>
							<td>$1,120</td>
						</tr>
					</tbody>
				</table>
				<a id="booking-form-button" href="#bookingForm" role="button" class="btn btn-medium btn-theme" data-toggle="modal" onClick="_gaq.push(['_trackEvent', 'BookingForm', 'View', 'ClickLink']);">Book</a>
			</div>
		</div>
	</section>

	<!-- end book -->

	<!-- start booking form -->
	<div id="bookingForm" class="modal hide fade" tabindex="-1" role="dialog" aria-labelledby="bookingForm" aria-hidden="tru">
		<div class="modal-header">
			<button type="button" class="close" data-dismiss="modal" aria-hidden="true">×</button>
    		<h3 id="myModalLabel">Book</h3>
		</div>

  		<div class="modal-body">
  		  	<form name="userform" ng-controller="Ctrl">
  		  		<h5  ng-hide="bookingSent">Booking details</h5>
  		  		<div class="row">
  		  			<div class="span8">
		  		  		<div class="span3" id="booking-input" ng-hide="bookingSent">
		  		  			<input type="text" placeholder="First name" ng-model="user.firstname" name="uFirstname" required>
					        <div ng-show="userform.uFirstname.$invalid" class='validation-error'>
					          <span ng-show="userform.uFirstname.$error.required">What's your first name?</span>
					        </div>

		  		  			<input type="text" placeholder="Last name" ng-model="user.lastname" name="uLastname" required>
					        <div ng-show="userform.uLastname.$invalid" class='validation-error'>
					          <span ng-show="userform.uLastname.$error.required">What's your last name?</span>
					        </div>

		  		  			<input type="email" placeholder="Email" ng-model="user.email" name="uEmail" required>
					        <div ng-show="userform.uEmail.$invalid" class='validation-error'>
					          <span ng-show="userform.uEmail.$error.required">What's your email?</span>
					          <span ng-show="userform.uEmail.$error.email">This is not a valid email.</span>
					        </div>

		  		  			<input type="tel" placeholder="Phone number" ng-model="user.phonenumber" name='uPhonenumber' required>
					        <div ng-show="userform.uPhonenumber.$invalid" class='validation-error'>
					          <span ng-show="userform.uPhonenumber.$error.required">What's your phone number?</span>
					        </div>
		  		  		</div>

		  		  		<div class="span4" ng-hide="bookingSent">
		  		  			<div id="nights">
		  		  				<p class="operator" ng-click="addNight()">+</p>
		  		  				<p class="operator minus" ng-click="minusNight()">-</p>
		  		  				<p class="big-form-value">{{user.nights}} nights</p>
		  		  			</div>
		  		  		</div>

		  		  		<div class="span4">
		  		  			<div id="startdate" ng-hide="bookingSent">
		  		  				<input ng-model="user.startdate" ui-date required>
		  		  				<p class="big-form-value">1st Night</p>
		  		  			</div>
		  		  		</div>
  		  			</div>
  		  		</div>

  		  		<div class="booking-sent" ng-show="bookingSent">
  		  			<p>Thanks {{user.firstname}} your booking was sent!</p>
  		  			<p>We'll be in touch soon, Greg.</p>
  		  		</div>

		  		<div class="modal-footer" ng-hide="bookingSent">
		  			<!-- <p id="totalcost">Total: ${{user.cost}}</p> -->
		  		  	<button class="btn btn-primary formbutton" ng-disabled="!userform.$valid" ng-click="sendBooking()">Book</button>
		  		</div>
  		  	</form>
  		</div>
	</div>
	<!-- end booking form -->

